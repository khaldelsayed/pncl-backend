{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "alwaysStrict": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "compileOnSave": true,
  "include": [
    "src/**/*.ts",
    "src/**/*.csv"
  ],
  "exclude": [
    "node_modules"
  ]
}
